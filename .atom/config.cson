"*":
  autosave:
    enabled: true
  core:
    telemetryConsent: "no"
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
  editor:
    fontFamily: "Inconsolata for Powerline, Menlo, Consolas, DejaVu Sans Mono, monospace"
    fontSize: 20
    scrollPastEnd: true
  "exception-reporting":
    userId: "17ed2d75-2536-4685-b577-f00046a26f98"
  "linter-shellcheck":
    enableNotice: true
  "linter-ui-default":
    panelHeight: 136
    showPanel: true
  "markdown-preview":
    useGitHubStyle: true
  "markdown-table-editor":
    formatOnSave: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
