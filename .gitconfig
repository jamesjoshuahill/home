[alias]
	br = branch -vv
	ci = commit
	co = checkout
	cp = cherry-pick
	di = diff
	ds = diff --staged
	lg = log --pretty=coloured
	lga = log --pretty=coloured --all --graph
	pf = push --force-with-lease
	pl = pull --rebase
	re = remote -v
	rs = restore --staged
	rb = rebase
	st = status
	sw = switch
[commit]
	verbose = true
[credential]
	helper = osxkeychain
[include]
	path = ~/.gitconfig.diff-so-fancy
	path = ~/.gitconfig.git-co-author
[pretty]
	coloured = format:%C(auto)%h%d %s %C(green)(%cr) %C(cyan)%an
[user]
	name = Josh Hill
	email = jamesjoshuahill@users.noreply.github.com
